{
    "multipart": [
        {   "apply": { "model": "simply_equine:block/stall_bars_1_post_ends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "simply_equine:block/stall_bars_1_post" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "simply_equine:block/stall_bars_1_cap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "simply_equine:block/stall_bars_1_cap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "simply_equine:block/stall_bars_1_cap_alt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "simply_equine:block/stall_bars_1_cap_alt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "simply_equine:block/stall_bars_1_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "simply_equine:block/stall_bars_1_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "simply_equine:block/stall_bars_1_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "simply_equine:block/stall_bars_1_side_alt", "y": 90 }
        }
    ]
}
